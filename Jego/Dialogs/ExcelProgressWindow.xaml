<Window x:Class="Jego.Dialogs.ExcelProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="Manual" 
        WindowStyle="None"
        AllowsTransparency="True"
        ResizeMode="NoResize"
        Background="Transparent" 
        Title="ExcelProgressWindow" Height="200" Width="400" Loaded="Window_Loaded" Unloaded="Window_Unloaded">
    <Grid>
        <Grid VerticalAlignment="Center" Height="80" Width="400" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="20"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <TextBlock Text="엑셀 문서를 생성하고 있습니다." Margin="10,0"/>
            <ProgressBar Name="progressbar" Grid.Row="1" Width="350" Height="20" Minimum="0" Maximum="100"  Margin="0,10"/>
        </Grid>
        
    </Grid>
</Window>
